{
  "name": "matcha",
  "version": "0.0.0",
  "description": "Matcha-ts: Simple learning-oriented typescript CLI",
  "main": "index.js",
  "scripts": {
    "test": "vitest",
    "build": "tsc && chmod +x dist/src/index.js && pkg ."
  },
  "bin": {
    "matcha": "dist/src/index.js"
  },
  "pkg": {
    "scripts": "dist/src/**/*.js",
    "targets": [
      "node16-win-x64",
      "node16-mac-x64",
      "node16-linux-x64"
    ],
    "outputPath": "bin"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "commander": "^12.1.0",
    "typescript": "^5.5.3"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.2",
    "vitest": "^2.0.2"
  }
}
